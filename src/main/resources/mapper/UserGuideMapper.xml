<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.choerodon.iam.infra.mapper.UserGuideMapper">

    <select id="queryUserGuideByMenuId" resultType="io.choerodon.iam.api.vo.UserGuideVO">
        select *
        from fd_user_guide fug
        where fug.menu_id = #{menuId}
    </select>
</mapper>
